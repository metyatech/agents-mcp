{
  "name": "@metyatech/agents-mcp",
  "version": "0.2.12",
  "description": "True multi-agent coding in your IDE. Spawn Claude, Codex, Gemini, and Cursor agents from a single MCP server.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agents-mcp": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metyatech/agents-mcp.git"
  },
  "homepage": "https://github.com/metyatech/agents-mcp",
  "bugs": {
    "url": "https://github.com/metyatech/agents-mcp/issues"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "node scripts/postinstall.mjs",
    "prepublishOnly": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "verify": "npm run lint && npm run format:check && npm run typecheck && npm test && npm run build",
    "verify-dist": "node scripts/verify-dist.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.13.0",
    "globals": "^15.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai-agents",
    "multi-agent",
    "claude-code",
    "codex",
    "gemini-cli",
    "cursor",
    "orchestration",
    "swarm"
  ],
  "author": "Swarmify <dev@swarmify.co>",
  "license": "MIT",
  "engines": {
    "node": ">=18.17.0"
  }
}
